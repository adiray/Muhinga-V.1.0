<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.muhingalayoutprototypes">
    <!-- To auto-complete the email text field in the email form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".AppNotifications"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SavedItems"></activity>
        <activity android:name=".SavedCategories" />
        <activity android:name=".ArtistSearch" />
        <activity android:name=".BookVenue" />
        <activity android:name=".Profile" />
        <activity android:name=".Login" />
        <activity android:name=".SignUp" />
        <activity android:name=".VenuesDetails" />
        <activity android:name=".Venues" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Houses" />
        <activity android:name=".HousesDetails" />
        <activity android:name=".Land" />
        <activity android:name=".LandDetails" />
        <activity android:name=".Music" />
        <activity android:name=".Artist" />
        <activity android:name=".Album" />
        <activity android:name=".PlayMusic" />

        <receiver android:name=".NotificationReceiver" />
    </application>

</manifest>